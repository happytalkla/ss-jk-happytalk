<!DOCTYPE configuration
		PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<package name="ht.domain"/>
	</typeAliases>
	<typeHandlers>
		<typeHandler handler="org.apache.ibatis.type.EnumOrdinalTypeHandler" javaType="java.math.RoundingMode"/>
		<typeHandler handler="org.apache.ibatis.type.ClobTypeHandler" jdbcType="CLOB" javaType="java.lang.String"/>
	</typeHandlers>
	<mappers>
		<mapper resource="mapper/Auth.xml" />
		<mapper resource="mapper/AutoCmp.xml" />
		<mapper resource="mapper/Batch.xml" />
		<mapper resource="mapper/Category.xml" />
		<mapper resource="mapper/Certification.xml" />
		<mapper resource="mapper/Channel.xml" />
		<mapper resource="mapper/Chat.xml" />
		<mapper resource="mapper/ChatManage.xml" />
		<mapper resource="mapper/Common.xml" />
		<mapper resource="mapper/Customer.xml" />
		<mapper resource="mapper/EndCtg.xml" />
		<mapper resource="mapper/HappyBot.xml" />
		<mapper resource="mapper/Manage.xml" />
		<mapper resource="mapper/Member.xml" />
		<mapper resource="mapper/Menu.xml" />
		<mapper resource="mapper/Notice.xml" />
		<mapper resource="mapper/Report.xml" />
		<mapper resource="mapper/Reporting.xml" />
		<mapper resource="mapper/Setting.xml" />
		<mapper resource="mapper/Skill.xml" />
		<mapper resource="mapper/Template.xml" />
		<mapper resource="mapper/Term.xml" />
		<mapper resource="mapper/Screen.xml" />
		<mapper resource="mapper/Codemgr.xml" />
		<mapper resource="mapper/Genesys.xml" />
		<mapper resource="mapper/legacy/MappingCustomer.xml" />
		<mapper resource="mapper/Genesys.xml" />
	</mappers>
</configuration>
